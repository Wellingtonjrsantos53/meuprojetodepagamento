{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/create_pix_payment",
      "methods": ["POST"],
      "dest": "/server.js"
    },
    {
      "src": "/create_card_payment",
      "methods": ["POST"],
      "dest": "/server.js"
    },
    {
      "src": "/payment_status/(.*)",
      "methods": ["GET"],
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}